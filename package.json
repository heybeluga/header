{
  "name": "@heybeluga/header",
  "version": "0.0.1",
  "description": "Common headers and footers for Beluga's webapps",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "pnpm build && node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -r dotenv/config -q dist/index.js\"",
    "lint": "eslint --ext .ts",
    "fix": "pnpm lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heybeluga/header.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heybeluga/header/issues"
  },
  "homepage": "https://github.com/heybeluga/header#readme",
  "types": "dist/index.d.ts",
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.3",
    "typescript": "^4.9.5"
  }
}
