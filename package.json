{
  "name": "@heybeluga/header",
  "version": "0.0.0",
  "description": "Common headers and footers for Beluga's webapps",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "pnpm build && node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -r dotenv-flow/config -q dist/index.js\"",
    "lint": "eslint --ext .ts",
    "fix": "pnpm lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heybeluga/header.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heybeluga/header/issues"
  },
  "homepage": "https://github.com/heybeluga/header#readme",
  "types": "dist/index.d.ts",
  "dependencies": {
    "dotenv-flow": "^3.3.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "eslint": "^8.50.0",
    "nodemon": "^2.0.22",
    "typescript": "^4.9.5"
  }
}
